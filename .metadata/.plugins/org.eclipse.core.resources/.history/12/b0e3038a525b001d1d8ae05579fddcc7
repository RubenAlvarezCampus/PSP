package Actividad_2_3_SincronizarHilos;

import java.util.concurrent.ThreadLocalRandom;

public class Ejercicio3_Productor_circunferencia extends Thread{
	
	private Ejercicio3_pila pila;
	
	public Ejercicio3_Productor_circunferencia(Ejercicio3_pila lista) {
		super();
		this.pila = lista;
		this.start();
	}

	@Override
	public void run() {
		/*Añadir una circunferencia*/
		int num = ThreadLocalRandom.current().nextInt(1,51);
		Ejercicio3_Circunferencia circunferencia = new Ejercicio3_Circunferencia(num);
		pila.añadir(circunferencia);
		
	}
}

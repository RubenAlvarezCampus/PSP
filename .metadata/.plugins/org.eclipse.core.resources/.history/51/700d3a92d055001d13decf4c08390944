package Colas;

import java.util.LinkedList;
import java.util.Queue;

public class BufferFrases {
	private Queue<String> cola;
	
	public BufferFrases() {
	cola = new LinkedList<String>();
	}
	
	public void poner(String frase) {
	cola.add(frase);
	}
	
	public String sacar() {
	if (cola.isEmpty()) {
	return null;
	}
	
	String frase = cola.remove();
	return frase;
	}
}

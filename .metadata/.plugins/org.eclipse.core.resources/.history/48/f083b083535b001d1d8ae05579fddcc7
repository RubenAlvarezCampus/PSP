package Actividad_2_3_SincronizarHilos;

public class Ejercicio3 {

	private static final int NUM_PRODUCTORES = 2;
	private static final int NUM_CONSUMIDORES = 1;
	
	public static void main(String[] args) {
		Ejercicio3_pila pila = new Ejercicio3_pila();
		
		for (int i = 0; i < NUM_PRODUCTORES; i++) {
			Ejercicio3_Productor Productor = new Ejercicio3_Productor(i,pila);
		}
		
		for (int i = 0; i < NUM_CONSUMIDORES; i++) {
			Ejercicio3_Consumidor Productor = new Ejercicio3_Consumidor(pila);
		}
	}
}
